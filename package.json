{
  "name": "mcp-gentest",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "clean": "rm -rf dist dist-cucumber node_modules package-lock.json yarn.lock",
    "build": "tsc -p tsconfig.build.json",
    "build:tests": "tsc -p tsconfig.tests.json",
    "build:all": "yarn build && yarn build:tests",
    "analyze": "yarn build && node dist/analyzer/index.js",
    "analyze:example": "yarn build && node dist/analyzer/index.js --project projects/sbg-ibizatechforum-landing",
    "features": "yarn build && node dist/prompt/index.js",
    "features:example": "yarn build && node dist/prompt/index.js --project projects/sbg-ibizatechforum-landing --include src/ --limit 3 --overwrite",
    "steps": "yarn build:tests && node dist-cucumber/steps/index.js",
    "steps:example": "yarn build:tests && node dist-cucumber/steps/index.js --project cucumber/features --overwrite",
    "runner": "yarn build && yarn build:tests && node dist/runner/index.js",
    "runner:example": "yarn build && yarn build:tests && node dist/runner/index.js --project projects/sbg-ibizatechforum-landing --include src/ --limit 3 --serve",
    "test:run": "yarn build:tests && cucumber-js cucumber/features --require dist-cucumber/step_definitions/**/*.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.1",
    "openai": "^5.11.0",
    "selenium-webdriver": "^4.34.0",
    "tree-kill": "^1.2.2",
    "yaml": "^2.8.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^12.1.0",
    "@types/chai": "^5.2.2",
    "@types/node": "^24.1.0",
    "@types/protractor": "^4.0.0",
    "@types/yargs": "^17.0.33",
    "chai": "^5.2.1",
    "dotenv": "^17.2.1",
    "protractor": "^7.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
